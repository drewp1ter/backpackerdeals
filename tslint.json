{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": [true],
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-unused-expression": false,
    "no-shadowed-variable": false,
    "variable-name": false,
    "member-access": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/routes.js"
    ]
  }
}
